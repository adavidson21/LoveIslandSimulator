<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.loveislandsimulator.controllers.HomeController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Love Island Game Show Simulator" textAlignment="CENTER">
      <font>
         <Font size="48.0" />
      </font>
      <VBox.margin>
         <Insets />
      </VBox.margin>
   </Text>
   <Button id="startBtn" defaultButton="true" mnemonicParsing="false" prefHeight="60.0" prefWidth="171.0" text="Start Game">
      <font>
         <Font size="24.0" />
      </font>
      <VBox.margin>
         <Insets top="100.0" />
      </VBox.margin>
   </Button>
   <Button id="helpBtn" mnemonicParsing="false" prefHeight="48.0" prefWidth="99.0" text="Help">
      <font>
         <Font size="18.0" />
      </font>
      <VBox.margin>
         <Insets top="100.0" />
      </VBox.margin>
   </Button>
</VBox>
